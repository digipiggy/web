{
  "version": 2,
  "public": false,
  "env": {
    "AWS_REG": "@aws_region",
    "AWS_KEY": "@aws_key",
    "AWS_SECRET": "@aws_secret",
    "WEBHOOK_ID": "@webhook_id",
    "PARTICLE_CLIENT_ID": "@particle_client_id",
    "PARTICLE_CLIENT_SECRET": "@particle_client_secret"
  },
  "builds": [
    {
      "src": "app/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "api/device.js",
      "use": "@now/node"
    },
    {
      "src": "api/webhooks.js",
      "use": "@now/node"
    }
  ],
  "alias": [
    "digipiggybank.com",
    "www.digipiggybank.com"
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/app/index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/app/favicon.ico"
    },
    {
      "src": "/touch-icon-iphone-retina.png",
      "dest": "/app/touch-icon-iphone-retina.png"
    },
    {
      "src": "/css/(?<file>.*)",
      "dest": "/app/css/$file"
    },
    {
      "src": "/js/(?<file>.*)",
      "dest": "/app/js/$file"
    },
    {
      "src": "/img/(?<file>.*)",
      "dest": "/app/img/$file"
    },
    {
      "src": "/api/device/(?<id>[^/]*)",
      "dest": "/api/device.js?deviceId=$id"
    },
    {
      "src": "/webhooks/device",
      "dest": "api/webhooks.js"
    }
  ]
}